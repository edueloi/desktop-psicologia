{
  "name": "psychdesk",
  "version": "1.0.0",
  "description": "Sistema de gest√£o para psicologia - Desktop",
  "main": "electron/main.js",
  "author": {
    "name": "Eduardo",
    "email": "contato@psychdesk.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/psychdesk"
  },
  "keywords": [
    "psicologia",
    "gestao",
    "consultorio",
    "pacientes",
    "electron",
    "desktop"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .",
    "start": "cross-env NODE_ENV=development electron .",
    "build": "tsc && vite build",
    "build:electron": "electron-builder",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed:run": "knex seed:run",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "@fullcalendar/list": "^6.1.10",
    "@fullcalendar/react": "^6.1.10",
    "@fullcalendar/timegrid": "^6.1.10",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@mui/x-data-grid": "^5.17.26",
    "@mui/x-date-pickers": "^5.0.0",
    "axios": "^1.3.0",
    "bcrypt": "^5.1.0",
    "better-sqlite3": "^8.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.0",
    "multer": "^1.4.5-lts.1",
    "pdfmake": "^0.2.7",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-router-dom": "^6.8.0",
    "uuid": "^9.0.0",
    "zod": "^3.20.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@hookform/resolvers": "^2.9.11",
    "@types/bcrypt": "^5.0.0",
    "@types/better-sqlite3": "^7.6.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/minimatch": "^6.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/pdfmake": "^0.2.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^22.3.27",
    "electron-builder": "^23.6.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.psychdesk.app",
    "productName": "PsychDesk",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "electron/**/*",
      "dist/**/*",
      "server/**/*",
      "knexfile.js",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "data",
        "to": "data",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  }
}
